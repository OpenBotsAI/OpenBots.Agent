<Window x:Class="OpenBots.Agent.Client.Forms.Dialog.MessageDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpenBots.Agent.Client.Forms.Dialog"
        mc:Ignorable="d"
        Title="" ResizeMode="NoResize" SizeToContent="Height" WindowStartupLocation="CenterOwner" Width="380">

    <Grid Name="grd_Main" Margin="17,10,17,30">
        <StackPanel Name="pnl_Message">
            <TextBlock x:Name="txtBlock_Message" Margin="0,5,0,5" FontSize="14" TextWrapping="Wrap"/>
        </StackPanel>
    </Grid>
</Window>
